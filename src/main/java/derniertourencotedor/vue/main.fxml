<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="derniertourencotedor.controler.MainControler">
   <children>
      <VBox alignment="CENTER" layoutX="7.0" layoutY="14.0">
         <children>
            <Label fx:id="nomTournee" text="Nom de la tournée">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
            <HBox spacing="10.0">
               <children>
                  <VBox spacing="20.0">
                     <children>
                        <ComboBox fx:id="selectTourne" prefHeight="26.0" prefWidth="176.0" />
                        <Button mnemonicParsing="false" onAction="#calculerTourner" prefHeight="26.0" prefWidth="175.0" text="Calculer coût" />
                        <HBox alignment="CENTER" spacing="5.0">
                           <children>
                              <Label alignment="CENTER" prefHeight="26.0" prefWidth="133.0" text="Coût de la tournée" underline="true">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                              <Label fx:id="champCout" text="Coût ">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <ListView fx:id="listVilles" prefHeight="282.0" prefWidth="363.0" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
